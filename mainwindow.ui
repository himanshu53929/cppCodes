/* Style for all QPushButtons except logout */
QPushButton {
    background-color: #03CEA4;       /* Teal green */
    color: white;                    /* White text */
    border-radius: 6px;               /* Rounded corners */
    padding: 6px 14px;                /* Inner padding */
    font-size: 16px;                  /* Text size */
    font-weight: bold;                /* Bold text */
    border: none;                     /* No border */
}

QPushButton:hover {
    background-color: #02b895;        /* Slightly darker teal on hover */
}

QPushButton:pressed {
    background-color: #029a7d;        /* Dark teal when pressed */
