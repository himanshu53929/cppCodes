    padding-bottom: 5px;
}

QPushButton:disabled {
    background-color: #95a5a6;        /* Greyed out */
    color: #7f8c8d;
}

/* Logout button retains its own style */
QPushButton#log_out_button {
    background-color: #e74c3c; 
    color: white; 
    border-radius: 8px;
    padding: 6px 14px;
    font-size: 14px;
    font-weight: bold;
    border: none;
}

QPushButton#log_out_button:hover {
    background-color: #c0392b;
}

